<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.OOPnewsfeedController">
   <children>
      <Label layoutX="230.0" layoutY="35.0" text="OOP Newsfeed">
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font>
      </Label>
      <TextArea fx:id="newPost" layoutX="53.0" layoutY="102.0" prefHeight="66.0" prefWidth="586.0" promptText="Write Something">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </TextArea>
      <Button layoutX="576.0" layoutY="178.0" mnemonicParsing="false" onAction="#PostIt" prefHeight="25.0" prefWidth="57.0" text="Post">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <VBox fx:id="myVBox" layoutX="53.0" layoutY="235.0" prefHeight="423.0" prefWidth="586.0" />
      <Button layoutX="613.0" layoutY="23.0" mnemonicParsing="false" onAction="#Reload" text="Reload ">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#Back" text="Back" />
   </children>
</AnchorPane>
